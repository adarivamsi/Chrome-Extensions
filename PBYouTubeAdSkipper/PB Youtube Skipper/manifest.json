{
    "name": "Passionate Burger YouTube Ad Skipper",
    "description": "This extension helps you to skip ads and any banner ads on the screen",
    "version": "0.1.6",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.youtube.com/*"],
        "js": ["/js/jquery-3.3.1.min.js","script.js"]
      }
    ],
    "browser_action": {
        "default_title": "PB YouTube Ad Skipper",
        "default_icon": "/logo/128.png"
    },
    "icons": { 
        "16": "/logo/16.png",
        "48": "/logo/48.png",
       "128": "/logo/128.png" 
    },
    "manifest_version": 2
  }